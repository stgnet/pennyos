#redhat-logos-60.0.14/anaconda/syslinux-splash.png: PNG image data, 640 x 300, 8-bit colormap, non-interlaced
anaconda/syslinux-splash.png: convert -size 320x70 -background none -fill white -font $FONT -gravity center label:"$NAME" name.png && convert -size 640x300 xc:$BACK $TEMP && composite -geometry +30+180 name.png $TEMP $TEMP && composite -geometry 64x64+530+180 $LOGO $TEMP $FILE
.: echo "END OF FILE"
